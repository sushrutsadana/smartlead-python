{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/docs",
            "dest": "api/index.py"
        },
        {
            "src": "/openapi.json",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "env": {
        "PYTHONPATH": "."
    }
} 